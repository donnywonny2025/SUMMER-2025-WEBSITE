{
  "name": "madison-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:visual": "playwright test tests/visual/visual.spec.js",
    "test:visual:baseline": "playwright test tests/visual/visual.spec.js --update-snapshots",
    "test:visual:report": "playwright show-report",
    "test:sam:baseline": "playwright test tests/visual/sam-baseline.spec.js",
    "test:compare": "playwright test tests/visual/comparison.spec.js",
    "rollback": "git reset --hard HEAD~1 && npm install",
    "safe-add": "npm run dev & sleep 3 && npm run test:visual",
    "component:add": "npx motion-primitives@latest add $1 && npm run safe-add",
    "component:test": "node tools/scripts/check-changes.js",
    "inspect": "node tools/scripts/element-inspector.js",
    "analyze-image": "node tools/scripts/image-inspector.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "autoprefixer": "^10.4.14",
    "clsx": "^2.1.1",
    "gsap": "^3.12.2",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "postcss": "^8.4.24",
    "puppeteer": "^24.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "vite": "^5.0.8",
    "sharp": "^0.33.0"
  }
}
